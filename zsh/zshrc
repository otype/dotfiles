# Path to your oh-my-zsh installation.
export ZSH=$HOME/.oh-my-zsh

# Set name of the theme to load.
# Look in ~/.oh-my-zsh/themes/
# Optionally, if you set this to "random", it'll load a random theme each
# time that oh-my-zsh is loaded.
ZSH_THEME="otype"

# Automatically upgrade oh-my-zsh without prompting you
DISABLE_UPDATE_PROMPT=true

# Which plugins would you like to load? (plugins can be found in ~/.oh-my-zsh/plugins/*)
# Custom plugins may be added to ~/.oh-my-zsh/custom/plugins/
# Example format: plugins=(rails git textmate ruby lighthouse)
# Add wisely, as too many plugins slow down shell startup.
plugins=(aws brew common-aliases colored-man-pages colorize docker encode64 github gpg-agent git git-extras history pyenv python rbenv ruby vi-mode)

# Source oh-my-zsh
source $ZSH/oh-my-zsh.sh

# Source zshoptions (for incremental search)
source $HOME/.zsh/zshoptions

# Source aliases files
source $HOME/.zsh/zshaliases

# Source zshenv files
source $HOME/.zsh/zshenv

PATH="/Users/hgschmidt/perl5/bin${PATH:+:${PATH}}"; export PATH;
PERL5LIB="/Users/hgschmidt/perl5/lib/perl5${PERL5LIB:+:${PERL5LIB}}"; export PERL5LIB;
PERL_LOCAL_LIB_ROOT="/Users/hgschmidt/perl5${PERL_LOCAL_LIB_ROOT:+:${PERL_LOCAL_LIB_ROOT}}"; export PERL_LOCAL_LIB_ROOT;
PERL_MB_OPT="--install_base \"/Users/hgschmidt/perl5\""; export PERL_MB_OPT;
PERL_MM_OPT="INSTALL_BASE=/Users/hgschmidt/perl5"; export PERL_MM_OPT;
