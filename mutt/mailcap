# MS Word documents
application/msword; open %s

# Images
image/jpg; open %s
image/jpeg; open %s
image/pjpeg; open %s
image/png; open %s
image/gif; open %s

# PDFs
application/pdf; open %s

# HTML
text/html;                      links %s; nametemplate=%s.html
text/html;                      links -dump %s; nametemplate=%s.html; copiousoutput

# TXT
text/plain; /Users/hgschmidt/bin/mvim %s

# Unidentified files
application/octet-stream; open %s
